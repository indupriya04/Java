public class R192110486{
   public static void main(String args[]) {
      int n1 = 15;
      int n2 = 85;
      int n3 = 7;
      if (n1 >= n2 && n1 >= n3){
         System.out.println( n1 + " is the maximum number.");
      }
      else if (n2 >= n1 && n2 >= n3){
         System.out.println( n2 + " is the maximum number.");
      }
      else{
         System.out.println( n3 + " is the maximum number.");
      }
   }
}
