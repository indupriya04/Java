import java.util.Scanner;
public class R192110486{
    public static void main(String[] args) {
        int n,i;
        Scanner in=new Scanner(System.in);
        System.out.println("enter a number=");
        n=in.nextInt();                
        for(i = 1; i <= 10; ++i)
        {
            System.out.printf("%d * %d = %d \n",n, i,n* i);
        }
    }
}
